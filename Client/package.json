{
  "name": "ultimate-trivia-client",
  "version": "0.0.1",
  "scripts": {
    "start": "parcel src/index.html --https --cert ../Server/UltimateTrivia/Certificate/localhost.crt --key ../Server/UltimateTrivia/Certificate/localhost.key",
    "marcel": "parcel src/index.html --https --cert ../Server/UltimateTrivia/Certificate/fullchain.pem --key ../Server/UltimateTrivia/Certificate/privkey.pem",
    "server": "dotnet run -p ../Server/UltimateTrivia/UltimateTrivia.csproj",
    "build": "parcel build src/index.html"
  },
  "dependencies": {
    "@microsoft/signalr": "^3.1.8",
    "avataaars": "^1.2.1",
    "faker": "^5.1.0",
    "lodash": "^4.17.20",
    "oidc-client": "^1.10.1",
    "parcel": "^1.12.4",
    "prettier": "^2.1.2",
    "react": "^16.14.0",
    "react-dom": "^16.13.1",
    "react-granim": "^1.1.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "sass": "^1.27.0"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "avoid"
  }
}
